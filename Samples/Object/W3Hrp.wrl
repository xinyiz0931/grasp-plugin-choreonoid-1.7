                  transparency	0
                }
              }
              geometry    IndexedFaceSet {
                coord     Coordinate {
                  point [
	             0.100  0.02 -0.02
	             0.100  0.02  0.02
	             0.100 -0.02 -0.02
	             0.100 -0.02  0.02
	            -0.100  0.02 -0.02
	            -0.100 -0.02 -0.02
	            -0.100 -0.02  0.02
	            -0.100  0.02  0.02
                  ]
                }
                coordIndex [
	          3  1  7  -1
	          7  6  3  -1
	          5  4  0  -1
	          0  2  5  -1
	          7  1  0  -1
	          0  4  7  -1
	          1  3  2  -1
	          2  0  1  -1
	          3  6  5  -1
	          5  2  3  -1
	          6  7  4  -1
	          4  5  6  -1
                ]
              }
            }
          ]
        }
      ]
    }
  ]
  joints [
    USE WAIST
  ]
  segments [
    USE BODY
  ]
  name "W3"
  version "1.1"
}

