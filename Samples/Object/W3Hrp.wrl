          centerOfMass 0.0 0.0 0.0
          momentsOfInertia [1 0 0 0 1 0 0 0 1]
          children [
            Shape {
              appearance	Appearance {
                material	DEF BANANA_light_or Material {
                  ambientIntensity	0.477124
                  diffuseColor	0.5 0.5 0.5
